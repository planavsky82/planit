{
  "name": "@planit/components",
  "version": "0.17.0",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/planit-container/planit-container.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "release-package": "func() { npm run build && npm version \"$1\" && npm publish --access public; }; func"
  },
  "dependencies": {
    "@stencil/core": "^2.0.1",
    "postcss": "^8.2.4",
    "tailwindcss": "^2.0.2"
  },
  "license": "MIT",
  "devDependencies": {
    "@stencil/postcss": "^2.0.0",
    "@types/autoprefixer": "^10.2.0",
    "autoprefixer": "^10.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planavsky82/planit.git"
  },
  "author": "Anthony Planavsky",
  "bugs": {
    "url": "https://github.com/planavsky82/planit/issues"
  },
  "homepage": "https://github.com/planavsky82/planit#readme"
}
